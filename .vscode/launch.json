    "version": "0.2.0",
    "configurations": [
        {"name":"Python Debugger: Current File",
            "type":"debugpy","request":"launch","program":"${file}","console":"integratedTerminal",
            "cwd":  "${workspaceFolder}"
        },
        {
            "name": "gdb",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/stm32-vector-xcp-slave.elf",
            "cwd":  "${workspaceRoot}",
            "MIMode": "gdb",
            "miDebuggerPath": "gdb-multiarch",
            "miDebuggerServerAddress": ":4242",
            "debugServerPath": "st-util",
            ""
        },
    ]
}